<entity-mappings
	xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	version="2.1">
	

	<entity class="pem.pemwebapp.domain.Profile">
		<named-query name="getProfile">
			<query>
				SELECT profile FROM Profile profile
			</query>
		</named-query>
	</entity>
	
	<entity class="pem.pemwebapp.domain.Session">
		<named-query name="listSessions">
			<query>
				SELECT session FROM Session session
				ORDER BY session.date ASC
			</query>
		</named-query>
	</entity>
	
</entity-mappings>
